{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "listing_norm",
  "type": "object",
  "required": [
    "listing_id",
    "idempotency_key",
    "title",
    "price_raw",
    "url"
  ],
  "properties": {
    "listing_id": {
      "type": "string",
      "minLength": 3
    },
    "idempotency_key": {
      "type": "string",
      "minLength": 6
    },
    "title": {
      "type": "string",
      "minLength": 3
    },
    "price_raw": {
      "type": "string",
      "minLength": 2
    },
    "price_huf": {
      "type": [
        "integer",
        "null"
      ],
      "minimum": 0
    },
    "currency": {
      "type": [
        "string",
        "null"
      ]
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "extracted_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    }
  },
  "additionalProperties": true
}